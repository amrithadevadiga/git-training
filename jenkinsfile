pipeline {
    agent any
    stages {
        stage("build") {
            steps {
                values = [:]
                currentWs = sh(returnStdout: true, script: 'pwd').trim()
                echo "build stage "
                echo "building"
                echo "${currentWs}"
                 
                   }
                }   
        stage("Test") {
            steps {
                echo "Test stage"
                   }
                }   
                
        stage("deploy") {
            steps {
                echo "build stage"
                   }
                }    
           }
      }
